@model IEnumerable<PDFManipulations.Models.FileDetailsModel>
<br />
<h2>Extracted images files</h2>
<br>
<table class="table table-bordered">
    <tr>
        <th class="col-md-4">
            @Html.DisplayName("Image")
        </th>
        <th class="col-md-4">
            @Html.DisplayName("Path")

        </th>
    </tr>
    @if (Model != null)
    {
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <img src="data:image/png;base64, @item.FileName" width="70%" height="20%" alt="" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FileTextContent)
                </td>
            </tr>
        }
    }
</table>
